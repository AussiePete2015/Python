import xlsxwriter
import pyodbc

# Database connection to GEO DATABASE
db_connection = pyodbc.connect('Driver={SQL Server Native Client 11.0};Server=BISYS-SQL-ORBIT\UAT;Database=ESRI_GISDATA;uid=########;pwd=#########')
db_cursor = db_connection.cursor()

sql_getgid = """
Select TOP 1[GID]
FROM [ESRI_GISDATA].[GISAdmin].[INCIDENTS_ALTERS]
LEFT OUTER JOIN [ESRI_GISDATA].[GISAdmin].[INCIDENTS_HAIL]
ON [INCIDENTS_ALTERS].[GEOEVENTID] = [INCIDENTS_HAIL].[GID] 
WHERE [INCIDENTS_ALTERS].[FEED_NAME] = 'Hail'
AND [INCIDENTS_HAIL].[HAILPROBABILITY] >= 90 AND VIL >= 50
GROUP BY GID, IMPACTED_POSTCODES, STARTDATETIME
HAVING MAX([INCIDENTS_ALTERS].[STARTDATETIME]) = MAX([INCIDENTS_HAIL].[EVENTDATETIME])
ORDER BY [INCIDENTS_ALTERS].[STARTDATETIME] DESC
"""

db_cursor.execute(sql_getgid)
getgid = db_cursor.fetchall()

# Extract String from List
geoeventid = [str(x) for x in getgid[0]]
gid = ','.join(geoeventid)

# Create a workbook and add a worksheet.
workbook = xlsxwriter.Workbook('D:\\downloads\\WFI_' + gid + '.xlsx')
worksheet = workbook.add_worksheet()

# Get Major Event GEOEVENTID for filename
sql_command = """
SET NOCOUNT ON

DECLARE @S varchar(max),
        @Split char(1),
        @X xml,
		@GID varchar(10),
		@filepath varchar(max),
		@sql as varchar(max),
		@impact_poc as varchar(5),
		@pc_intersect as varchar(25),
		@Riskkey as varchar(25),
		@cnt as int = 0,
		@poly_geom  geometry,
		@pif_geom geometry,
		@brand as varchar(5)

SET @S = ' '
SET @X = ' '

Select Top 1 @GID=[GID]
FROM [ESRI_GISDATA].[GISAdmin].[INCIDENTS_ALTERS]
LEFT OUTER JOIN [ESRI_GISDATA].[GISAdmin].[INCIDENTS_HAIL]
ON [INCIDENTS_ALTERS].[GEOEVENTID] = [INCIDENTS_HAIL].[GID] 
WHERE [INCIDENTS_ALTERS].[FEED_NAME] = 'Hail'
AND [INCIDENTS_HAIL].[HAILPROBABILITY] >= 90 AND VIL >= 50
GROUP BY GID, IMPACTED_POSTCODES, STARTDATETIME
HAVING MAX([INCIDENTS_ALTERS].[STARTDATETIME]) = MAX([INCIDENTS_HAIL].[EVENTDATETIME])
ORDER BY [INCIDENTS_ALTERS].[STARTDATETIME] DESC

select @S = [IMPACTED_POSTCODES] , @Split = ',' from 
(
Select TOP 1[IMPACTED_POSTCODES]
FROM [ESRI_GISDATA].[GISAdmin].[INCIDENTS_ALTERS]
LEFT OUTER JOIN [ESRI_GISDATA].[GISAdmin].[INCIDENTS_HAIL]
ON [INCIDENTS_ALTERS].[GEOEVENTID] = [INCIDENTS_HAIL].[GID] 
WHERE [INCIDENTS_ALTERS].[FEED_NAME] = 'Hail'
AND [INCIDENTS_HAIL].[HAILPROBABILITY] >= 90 AND VIL >= 50
GROUP BY GID, IMPACTED_POSTCODES, STARTDATETIME
HAVING MAX([INCIDENTS_ALTERS].[STARTDATETIME]) = MAX([INCIDENTS_HAIL].[EVENTDATETIME])
ORDER BY [INCIDENTS_ALTERS].[STARTDATETIME] DESC
) AS ROW

SELECT @X = CONVERT(xml,' <root> <s>' + REPLACE(@S,@Split,'</s> <s>') + '</s>   </root> ');

SELECT [impacted_postcode] = T.c.value('.','varchar(20)') 
INTO #TempTbl
FROM @X.nodes('/root/s') T(c)

SELECT @cnt = count(*) from #TempTbl

Set @brand = 'DI'

DECLARE postcode_cursor CURSOR FOR 
SELECT impacted_postcode from #TempTbl

OPEN postcode_cursor
Fetch next from postcode_cursor into @impact_poc
While @@FETCH_STATUS = 0
Begin

	DECLARE PCIntersect_cursor CURSOR FOR
	SELECT [RISK_KEY] FROM [GISAdmin].[FC_PIFALLBRANDS] WHERE [BRAND] = @brand AND [POSTCODE] = @impact_poc
	
	Select @cnt = count([RISK_KEY]) FROM [GISAdmin].[FC_PIFALLBRANDS] WHERE [BRAND] = @brand AND [POSTCODE] = @impact_poc

	OPEN PCIntersect_cursor
	Fetch next from PCIntersect_cursor into @pc_intersect
	WHILE @cnt >= 1
	Begin	
		Set @cnt = @cnt - 1
				
		Select @poly_geom = SHAPE.STAsText()
		from [GISAdmin].[INCIDENTS_ALTERS]
		where GEOEVENTID = @GID

		SELECT @pif_geom = SHAPE.STAsText() 
		FROM [GISAdmin].[FC_PIFALLBRANDS]
		where [BRAND] = @brand
		AND [POSTCODE] = @impact_poc
		And [RISK_KEY] = @pc_intersect

		While (select @poly_geom.STIntersects(@pif_geom) as IntersectValue) = 1
		Begin
			SELECT BRAND,
				LINE_OF_BUSINESS,
				POLICY_KEY,
				RISK_KEY,
				POLICY_NUMBER,
				SUBCOMPANY,
				GROUP_,
				COVERAGE,
				POLICY_INFORCE_DATE,
				POLICY_START_DATE,
				POLICY_END_DATE,
				POLCOUNT,
				RISKCOUNT,
				SUM_INSURED,
				REGISTRATION,
				MAKE,
				MODEL,
				YEAR_MANUFACTURED,
				TOWN,
				POSTCODE,
				STATE_,
				STREET_NUMBER,
				STREET_NAME,
				STREET_SUFFIX,
				STREET_OTHERDETAILS,
				GEOCODED_TOWN,
				GEOCODED_POSTCODE,
				GEOCODED_STATE,
				GEOCODED_STREET_NAME,
				INSURED_NAME,
				INSURED_HOMEPHONE,
				INSURED_WORKPHONE,
				INSURED_MOBEPHONE,
				INSURED_OTHERPHONE,
				INSURED_EMAIL,
				SECOND_INSURED_NAME,
				SECOND_INSURED_HOMEPHONE,
				SECOND_INSURED_WORKPHONE,
				SECOND_INSURED_MOBEPHONE,
				SECOND_INSURED_EMAIL,
				AGENCY_NAME,
				BROKER,
				FLOOD_FLAG,
				FLOOD_PREMIUM,
				FLOOD_COVERAGE,
				FLOOD_RISK_CONTENTS,
				FLOOD_RISK_BUILDING,
				BUSHFIRE_RATING,
				EMBARGO,
				YEAR_CONSTRUCTED,
				CONSTRUCTION_TYPE,
				ROOF_TYPE,
				WALLTYPE,
				FLOORTYPE,
				UNIT_FLAG,
				HH_HIGH_RISK_AREA_FLAG,
				POLICY_STATUS,
				POLICY_PAID,
				NEWBUSINESS_RENEWAL
			FROM [GISAdmin].[FC_PIFALLBRANDS] 
			WHERE [BRAND] = @brand 
			AND [POSTCODE] = @impact_poc
			AND [POLICY_KEY] = @pc_intersect
			--Select @GID AS geoeventid , @impact_poc as postcode, @pc_intersect as risk_key
		END
		Fetch next from PCIntersect_cursor into @pc_intersect
	END
	
	Fetch next from postcode_cursor into @impact_poc 
	close PCIntersect_cursor
	deallocate PCIntersect_cursor
END
close postcode_cursor
deallocate postcode_cursor
"""

db_cursor.execute(sql_command)
rows = db_cursor.fetchall()

print rows

xlrow = 1
xlcol = 0

